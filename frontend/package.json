    "test": "vitest",
    "coverage": "vitest --coverage",
    "cypress": "cypress open",
    "lint": "eslint .",
    "lint:fix": "eslint --fix",
    "format": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc",
    "build-old": "vite build && compress-cra && (rm -f src-maps-build/*.map) && (cp -r ./build/static/js/ ./src-maps-build/) && (rm ./build/static/js/*.map) && (export REACT_APP_SENTRY_RELEASE=$(git rev-parse --short HEAD); node sentry-upload.js)",
    "build": "node scripts/build.js",
    "build-prod": "export NODE_OPTIONS=--max-old-space-size=8192 && npm run build",
    "postbuild": "echo 'build done'",
    "build-maps": "react-scripts build",
    "badly-functioning-purge": " purgecss --css build/static/css/*.css --content build/index.html build/static/js/*.js --output build/static/css",
    "analyze": "source-map-explorer 'build/static/js/*.js' --html analysis.html",
    "test:cov": "npm test -- --coverage --watchAll=false",
    "test:debug": "react-scripts --inspect-brk test --runInBand",
    "playwright-test": "playwright test --project=chromium --headed --ui",
    "eject": "react-scripts eject"
  },
