{
    "containerDefinitions": [
        {
            "name": "dev-mentalyc-app-task",
            "image": "058264084218.dkr.ecr.us-west-1.amazonaws.com/application-repo:mentalyc-app1",
            "cpu": 0,
            "portMappings": [
                {
                    "containerPort": 8000,
                    "hostPort": 8000,
                    "protocol": "tcp"
                }
            ],
            "essential": true,
            "environment": [
                {
                    "name": "REDIS_HOST",
                    "value": "clustercfg.mentalyc-redis.vddx8f.usw1.cache.amazonaws.com:6379"
                },
                {
                    "name": "DB_USER",
                    "value": "test"
                },
                {
                    "name": "MEMCACHED_HOST",
                    "value": "mentalyc-memcached.vddx8f.cfg.usw1.cache.amazonaws.com:11211"
                },
                {
                    "name": "DISABLE_PHONE_TEST_DEPLOYMENT",
                    "value": "1"
                },
                {
                    "name": "OPENAI_API_KEY",
                    "value": ""
                },
                {
                    "name": "DB_HOST",
                    "value": "mydatabase1.cv48k0cm6kzn.us-west-1.rds.amazonaws.com"
                },
                {
                    "name": "DB_DATABASE",
                    "value": "mentalyc"
                },
                {
                    "name": "DB_PASSWORD",
                    "value": "hellotest"
                }
            ],
            "mountPoints": [],
            "volumesFrom": [],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/dev-mentalyc-app-log",
                    "awslogs-region": "us-west-1",
                    "awslogs-stream-prefix": "ecs"
                }
            },
            "systemControls": []
        }
    ],
    "family": "dev-mentalyc-app-task",
    "executionRoleArn": "arn:aws:iam::058264084218:role/dev-mentalyc-app-execution-role",
    "networkMode": "awsvpc",
    "cpu": "512",
    "memory": "1024",
    "tags": [
        {
            "key": "Name",
            "value": "dev-mentalyc-app-task"
        }
    ]
}